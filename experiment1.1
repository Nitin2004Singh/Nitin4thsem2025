class Animal:
    def makeSound(self):
        print("Some generic animal sound")
class Dog(Animal):
    def makeSound(self):
        print("Woof")
class Cat(Animal):
    def makeSound(self):
        print("Meow")
def demonstrate_polymorphism():
    animal1 = Dog()  
    animal2 = Cat() 
    animal1.makeSound()  
    animal2.makeSound()
demonstrate_polymorphism()
